#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    double num[100010];
    for (int i = 0; i < n; i++)
        scanf("%lf", &num[i]);
    double sum = 0;
    for (int i = 0; i < n; i++)
    {
        sum = sum + (num[i] * (n - i) * (i + 1));
    }
    printf("%.2f",sum);
    system("pause");
    return 0;
}
