#include <bits/stdc++.h>
using namespace std;


int main()
{
   int n;
   cin>>n;
   string s;
   for(int i=0;i<n;i++)
   {
       cin>>s;
       int len = s.length();
        int a = stoi(s.substr(0, len/2));
        int b = stoi(s.substr(len/2));
        if (a * b != 0 && stoi(s) % (a * b) == 0)
       cout<<"Yes"<<endl;
       else
       cout<<"No"<<endl;
   }
    system("pause");
    return 0;
}
