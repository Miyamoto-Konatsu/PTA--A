#include <bits/stdc++.h>
using namespace std;

string a,b;

int ccc[128];
int main()
{
    getline(cin,a);
    getline(cin,b);
    for(int i=0;i<b.size();i++)
      ccc[int(b[i])]=1;
    for(int i=0;i<a.size();i++)
        if(!ccc[int(a[i])])
        {
            cout<<a[i];
        }
    system("pause");
    return 0;
}
